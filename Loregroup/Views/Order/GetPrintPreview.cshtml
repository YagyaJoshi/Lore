                                            </tr>
                                        }
                                        <tr>
                                            <td colspan="3"><b>Tax/VAT</b></td>
                                            @*<td>@(Model.Tax * Model.Amount / 100)</td>*@
                                            @if (Model.CustomerModel.CurrencyName == "EURO")
                                            {
                                                <td colspan="2"> €  @Math.Round((Model.Tax * (Model.Amount + Model.ShippingCharge) / 100), 2) </td>
                                            }
                                            else if (Model.CustomerModel.CurrencyName == "GBP")
                                            {
                                                <td colspan="2"> £  @Math.Round((Model.Tax * (Model.Amount + Model.ShippingCharge) / 100), 2) </td>
                                            }
                                            else
                                            {
                                                <td colspan="2"> $  @Math.Round((Model.Tax * Model.Amount / 100), 2) </td>
                                            }
                                        </tr>
                                        <tr>
                                            <td colspan="3"><b>Total</b></td>
                                            @* <td> @Model.TotalAmount </td>*@

                                            @if (Model.CustomerModel.CurrencyName == "EURO")
                                            {
                                                <td colspan="2"> €  @Model.TotalAmount </td>
                                            }
                                            else if (Model.CustomerModel.CurrencyName == "GBP")
                                            {
                                                <td colspan="2"> £  @Model.TotalAmount </td>
                                            }
                                            else
                                            {
                                                <td colspan="2"> $ @Model.TotalAmount </td>
                                            }
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </td>
                    </tr>

                </tbody>
            </table>
        </div>
    </div>
</body>
</html>


<script type="text/javascript">

    function printDiv(divName) {
        var printContents = document.getElementById(divName).innerHTML;
        var originalContents = document.body.innerHTML;

        document.body.innerHTML = printContents;

        window.print();

        document.body.innerHTML = originalContents;
    }

    function Close() {
        window.close();
    }

</script>
