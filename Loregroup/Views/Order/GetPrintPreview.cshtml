            }

            .header {
                font-size: 15px;
            }

            .address-block {
                width: 180px;
                font-size: 11px;
            }
        }

        @@media (max-width:658px) {
            .address-block {
                width: 140px;
                font-size: 9px;
            }

            .order-form-text {
                font-size: 12px;
            }
        }

        @@media (max-width:595px) {
            .product td th {
                padding: 0 4px;
            }
        }

        @@media (max-width:551px) {
            .logo-header {
                width: 600px;
            }

